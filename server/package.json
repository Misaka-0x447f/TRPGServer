  "devDependencies": {
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "@types/ws": "^6.0.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "tsc-watch": "^1.1.37",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "uuid": "^3.3.2",
    "ws": "^6.1.4"
  }
